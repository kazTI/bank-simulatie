<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
          prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label text="ErrorMessages" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2"
               GridPane.valignment="BOTTOM"/>
        <Button alignment="CENTER" mnemonicParsing="false" text="Back" GridPane.rowIndex="2"
                GridPane.valignment="BOTTOM"/>
        <Button mnemonicParsing="false" text="Logout" GridPane.columnIndex="2" GridPane.halignment="RIGHT"
                GridPane.rowIndex="2" GridPane.valignment="BOTTOM"/>
        <Label text="Instructions" GridPane.columnIndex="1" GridPane.halignment="CENTER"/>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1"
              GridPane.halignment="CENTER" GridPane.rowIndex="1">
            <children>
                <Button mnemonicParsing="false" text="Option 1"/>
                <Button alignment="CENTER" mnemonicParsing="false" text="Option two"/>
                <Button mnemonicParsing="false" text="3rd option"/>
            </children>
        </VBox>
    </children>
</GridPane>
